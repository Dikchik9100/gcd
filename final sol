#include <numeric>  // for std::gcd

class Solution {
public:
    string gcdOfStrings(string str1, string str2) {
        // If concatenations don't match, there is no common divisor string
        if (str1 + str2 != str2 + str1) {
            return "";
        }

        int lenGCD = std::gcd((int)str1.length(), (int)str2.length());
        return str1.substr(0, lenGCD);
    }
};
